<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">
	<changeSet id="202508181000-1" author="user">
		<createTable tableName="ItemNodeSupply">
			<column name="nodeinventorykey" type="varchar(32)">
				<constraints nullable="false" />
			</column>
			<column name="supplytype" type="varchar(16)">
				<constraints nullable="false" />
			</column>
			<column name="quantity" type="INT" defaultValue="0">
				<constraints nullable="false" />
			</column>
			<column name="supplydate" type="DATE">
				<constraints nullable="false" />
			</column>
			<column name="itemnodesupplykey" type="varchar(32)">
				<constraints primaryKey="true" nullable="false" />
			</column>
		</createTable>
   </changeSet>
</databaseChangeLog>