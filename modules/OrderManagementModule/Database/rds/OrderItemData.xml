<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">
	<changeSet id="202508181000-1" author="user">
		<createTable tableName="OrderItemData">
			<column name="orderno" type="varchar(40)">
				<constraints nullable="false" />
			</column>
			<column name="lineno" type="INT">
				<constraints nullable="false" />
			</column>
			<column name="itemid" type="varchar(40)">
				<constraints nullable="false" />
			</column>
			<column name="status" type="INT">
				<constraints nullable="false" />
			</column>
			<column name="qty" type="INT">
				<constraints nullable="false" />
			</column>
			<column name="OrderItemKey" type="varchar(32)">
				<constraints primaryKey="true" nullable="false" />
			</column>
		</createTable>
		<createIndex indexName="orderitemdata_ind1" tableName="OrderItemData">
			<column name="orderno"/>
		</createIndex>
   </changeSet>
</databaseChangeLog>