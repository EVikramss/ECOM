<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">
	<changeSet id="202508181000-1" author="user">
		<createTable tableName="OrderAddress">
			<column name="orderno" type="varchar(40)">
				<constraints nullable="false" />
			</column>
			<column name="country" type="varchar(3)">
				<constraints nullable="false" />
			</column>
			<column name="city" type="varchar(16)">
				<constraints nullable="false" />
			</column>
			<column name="state" type="varchar(16)">
				<constraints nullable="false" />
			</column>
			<column name="addressline1" type="varchar(64)">
				<constraints nullable="false" />
			</column>
			<column name="addressline2" type="varchar(64)">
				<constraints nullable="false" />
			</column>
			<column name="addresskey" type="varchar(32)">
				<constraints primaryKey="true" nullable="false" />
			</column>
		</createTable>
		<createIndex indexName="orderaddress_ind1" tableName="OrderAddress">
			<column name="orderno"/>
		</createIndex>
   </changeSet>
</databaseChangeLog>